{
  "name": "Bronze-Silver-Gold Pipeline",
  "tasks": [
    {
      "task_key": "bronze",
      "notebook_task": {
        "notebook_path": "/Projects/HealthcareFIN/bronze/bronze_notebook"
      },
      "job_cluster_key": "job_cluster"
    },
    {
      "task_key": "silver",
      "depends_on": [
        {"task_key": "bronze"}
      ],
      "notebook_task": {
        "notebook_path": "/Projects/HealthcareFIN/silver/silver_notebook"
      },
      "job_cluster_key": "job_cluster"
    },
    {
      "task_key": "gold",
      "depends_on": [
        {"task_key": "silver"}
      ],
      "notebook_task": {
        "notebook_path": "/Projects/HealthcareFIN/gold/gold_notebook"
      },
      "job_cluster_key": "job_cluster"
    }
  ],
  "job_clusters": [
    {
      "job_cluster_key": "job_cluster",
      "new_cluster": {
        "spark_version": "13.3.x-scala2.12",
        "node_type_id": "Standard_DS3_v2",
        "num_workers": 2
      }
    }
  ]
}
